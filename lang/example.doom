END: 511
MAIN:
  or %2, %0, %0     # Initial address to load from
L0:
  or %1, $1, %0     # Load syscall
  syscall           # Load data from address in %2
  or %ax, %3, %0    # Move loaded data into %ax
  add %ax, %ax, $5
  or %1, $2, %0     # Save syscall
  or %3, %ax, %0
  syscall           # Save data in %3 into address in %2
  add %2, %2, $1    # Increment address
  xor %bx, %2, .END # Set %bx to zero if %2 == 511
  jnz .L0, %bx
  or %1, %0, %0     # Halt syscall
  syscall
